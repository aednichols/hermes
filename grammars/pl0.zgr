{
  "ll1": {
    "start": "program",
    "rules": [
      "program := block + 'dot'",
      "block := block_const + block_var + block_proc + statement",
      "block_const := 'const' + ident + 'equal' + number + block_const1 | ε",
      "block_const1 := 'comma' + ident + 'equal' + number | ε",
      "block_var := 'var' + ident + block_var1 + 'semi' | ε",
      "block_var1 := 'comma' + ident | ε",
      "block_proc := 'procedure' + ident + 'semi' + block + 'semi' + block_proc | ε",
      "statement := ident + 'assign' + expression | 'call' + ident | 'begin' + statement + statement_sub1 + 'end' | 'if' + condition + 'then' + statement | 'while' + condition + 'do' + statement",
      "statement_sub1 := 'semi' + statement | ε",
      "condition := 'odd' + expression | expression + op + expression",
      "op := 'equal' | 'pound' | 'lt' | 'lteq' | 'gt' | 'gteq'",
      "expression := unary_add_op + term + expression_sub1",
      "unary_add_op := add_op | ε",
      "expression_sub1 := add_op + expression_sub1 | ε",
      "add_op := 'plus' | 'minus'",
      "mul_op := 'times' | 'divide'",
      "term := factor + term_sub1",
      "term_sub1 := mul_op + term_sub1 | ε",
      "factor := ident | number | 'lparen' +  expression + 'rparen'",
      "ident := 'ident'",
      "number := 'number'"
    ]
  }
}
