grammar {
  lexer {
    partials {
      r'[\u266d-\u266f]' -> _music
    }
    r'm:({%_music%})' -> :music[1]
    r'"((?:[^"\\]|\\["\\/bfnrt]|\\u[0-9A-Fa-f]{4})*)"' -> :string[1]
    r'a' -> :a
    r'b' -> :b
    r'\{' -> :lbrace @blah
    r'\n+' -> :newline
    r' ' -> :space
    mode<blah> {
      r'blah' -> :blah
      r'}' -> :rbrace %pop
    }
  }
}
