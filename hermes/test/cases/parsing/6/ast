(Program:
  statements=[
    (Assign:
      var=identifier,
      val=(Assign:
        var=identifier,
        val=n
      )
    ),
    (For:
      decl=(Assign:
        var=identifier,
        val=n
      ),
      cond=None,
      iter=(Assign:
        var=identifier,
        val=(Add:
          lhs=s,
          rhs=n
        )
      ),
      body=(Assign:
        var=identifier,
        val=(Add:
          lhs=n,
          rhs=(Divide:
            lhs=(Multiply:
              lhs=s,
              rhs=n
            ),
            rhs=s
          )
        )
      )
    )
  ]
)
