(Program:
  statements=[
    (Assign:
      lhs=identifier,
      rhs=(Assign:
        lhs=identifier,
        rhs=n
      )
    ),
    (For:
      decl=(Assign:
        lhs=identifier,
        rhs=n
      ),
      cond=None,
      iter=(Assign:
        lhs=identifier,
        rhs=(Add:
          lhs=s,
          rhs=n
        )
      ),
      body=(Assign:
        lhs=identifier,
        rhs=(Add:
          lhs=n,
          rhs=(Multiply:
            lhs=identifier,
            rhs=(Power:
              lhs=n,
              rhs=s
            )
          )
        )
      )
    ),
    (Add:
      lhs=identifier,
      rhs=identifier
    )
  ]
)
